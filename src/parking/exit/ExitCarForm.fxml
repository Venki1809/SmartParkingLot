<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="250.0" prefWidth="350.0" xmlns="http://javafx.com/javafx/22" xmlns:fx="http://javafx.com/fxml/1" fx:controller="parking.exit.ExitCarFormController">
    <children>
        <Label layoutX="50.0" layoutY="50.0" text="Vehicle Number:" />
        <TextField fx:id="vehicleNumberField" layoutX="198.0" layoutY="45.0" prefHeight="26.0" prefWidth="104.0" />

        <Button layoutX="192.0" layoutY="125.0" onAction="#handleSubmit" prefHeight="26.0" prefWidth="74.0" style="-fx-background-color: #4a84f7;" text="Submit" textFill="WHITE">
         <font>
            <Font name="System Bold" size="12.0" />
         </font></Button>
      <Button layoutX="72.0" layoutY="125.0" onAction="#handleCancel" mnemonicParsing="false" prefHeight="26.0" prefWidth="74.0" style="-fx-background-color: #4a84f7;" text="Cancel" textFill="WHITE">
         <font>
            <Font name="Bookman Old Style Bold" size="12.0" />
         </font>
      </Button>
    </children>
</AnchorPane>
